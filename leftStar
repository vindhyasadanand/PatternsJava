/*
task is to print
*
* *
* * *


*/

public class Main
{
	public static void main(String[] args) {
	  int n=3;
    //it will run up to number of rows
	  for(int i=1;i<=n;i++)
	  {    //it will run up to number of rows
	      for(int j=1;j<=i;j++)
	      {
	          System.out.print("*" + " ");
	      }
	         System.out.println();
	  }
	}
}
